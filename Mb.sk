#사용변수
#{현재점령중} (거점이먹어져있는팀)
#{점령상태} (지금밟고있는상태)
#: 0 = 경합 -1은 블루가 세  1은 레드가 우세
#{점령게이지.red/blue} (점령%게이지%)
#{추가시간활성화.red/blue} (이 팀의 추가시간부여)
#{추가시간.red/blue} (이 팀의 추가시간)

#{강탈게이지} -80 ~ 0 까지는 블루 0 ~ 80까지 레드 0은 중립
#{점령경과시간.red/blue}
every 0.2 seconds:
	if {게임진행} is true:
		if {현재점령중} is -1:
			if {점령상태} is 1: #레드가 거점 점령중
				set {점령경과시간.red} to 30
				if {강탈게이지} < 40:
					add 1 to {강탈게이지}
					execute console command "/execute as @a at @s run playsound minecraft:entity.experience_orb.pickup master @a ~ ~ ~ 0.5 2"
					stop
				if {강탈게이지} is 40:
					set {현재점령중} to 1
					set {추가시간활성화.red} to false
					execute console command "/execute as @a at @s run playsound minecraft:entity.wither.break_block master @a ~ ~ ~ 0.4 1"
					set {강탈게이지} to 0
					stop
			if {점령경과시간.red} is 0:
				if {강탈게이지} > 0:
					subtract 1 from {강탈게이지}
					execute console command "/execute as @a at @s run playsound minecraft:entity.experience_orb.pickup master @a ~ ~ ~ 0.5 2"
					stop
				if {강탈게이지} <= 0:
					stop
			if {점령상태} is 0 or -1:
				if {점령경과시간.red} > 0:
					subtract 1 from {점령경과시간.red}
				if {점령경과시간.red} is 0:
					stop
		if {현재점령중} is 1: 
			if {점령상태} is -1: #블루가 거점 점령중
				set {점령경과시간.blue} to 30
				if {강탈게이지} > -40:
					subtract 1 from {강탈게이지}
					execute console command "/execute as @a at @s run playsound minecraft:entity.experience_orb.pickup master @a ~ ~ ~ 0.5 2"
					stop
				if {강탈게이지} is -40:
					set {현재점령중} to -1
					set {추가시간활성화.blue} to false
					execute console command "/execute as @a at @s run playsound minecraft:entity.wither.break_block master @a ~ ~ ~ 0.4 1"
					set {강탈게이지} to 0
					stop
			if {점령경과시간.blue} is 0:
				if {강탈게이지} < 0:
					add 1 to {강탈게이지}
					execute console command "/execute as @a at @s run playsound minecraft:entity.experience_orb.pickup master @a ~ ~ ~ 0.5 2"
					stop
				if {강탈게이지} is 0:
					stop
			if {점령상태} is 0 or 1:
				if {점령경과시간.blue} > 0:
					subtract 1 from {점령경과시간.blue}
				if {점령경과시간.blue} is 0:
					stop
		if {현재점령중} is 0:
			if {점령상태} is -1: #블루가 거점 점령중
				set {점령경과시간.blue} to 30
				if {강탈게이지} > -40:
					subtract 1 from {강탈게이지}
					stop
