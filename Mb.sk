command /레드승리:
	trigger:
		set {게임진행} to false
		send title "레드승리" to all players
		add 1 to {레드팀스코어}
		wait 2 seconds
		send title "현재 스코어 &9%{블루팀스코어}% &f: &4%{레드팀스코어}%"
		if {레드팀스코어} is 2:
			execute console command "/mb win red"
			execute console command "/execute at @a run playsound minecraft:entity.ender_dragon.ambient master @a ~ ~ ~ 0.6 1.4"
		if {레드팀스코어} < 2:
			execute console command "/mb next"
command /블루승리:
	trigger:
		set {게임진행} to false
		send title "블루승리" to all players
		add 1 to {블루팀스코어}
		wait 2 seconds
		send title "현재 스코어 &9%{블루팀스코어}% &f: &4%{레드팀스코어}%"
		if {블루팀스코어} is 2:
			execute console command "/mb win blue"
			execute console command "/execute at @a run playsound minecraft:entity.ender_dragon.ambient master @a ~ ~ ~ 0.6 1.4"
		if {블루팀스코어} < 2:
			execute console command "/mb next"
		



