#게임환경 설정
on damage:
	if {게임진행} is true:
		if victim is player:
			if attacker is player:
				if {red.%attacker%} is true:
					if {red.%victim%} is true: #같은팀을 공격했을때
						cancel event
					if {blue.%victim%} is true: #다른팀을 공격했을때
						if {hitdelay.%attacker%.%victim%} is 0:
							if {직업.냉술사.%attacker%} is true:
								set {hitdelay.%attacker%.%victim%} to {attackdelay.%attacker%}
								set maximum damage delay of victim to 0.1 seconds
								chance of 30:
									set {냉술사슬로우.%victim%} to 3
								stop
							if {직업.냉술사.%attacker%} is false:
								set {hitdelay.%attacker%.%victim%} to {attackdelay.%attacker%}
								set maximum damage delay of victim to 0.1 seconds
								if attacker's tool is iron axe:
									set damage to damage + {버서커추가뎀.%attacker%}
								stop
						if {hitdelay.%attacker%.%victim%} > 0:
							cancel event
						
							
				if {blue.%attacker%} is true:
					if {blue.%victim%} is true: #같은팀을 공격했을때
						cancel event
					if {red.%victim%} is true: #다른팀을 공격했을때
						if {hitdelay.%attacker%.%victim%} is 0:
							if {직업.냉술사.%attacker%} is false:
								set {hitdelay.%attacker%.%victim%} to {attackdelay.%attacker%}
								set maximum damage delay of victim to 0.1 seconds
								if attacker's tool is iron axe:
									set damage to damage + {버서커추가뎀.%attacker%}
								stop
							if {직업.냉술사.%attacker%} is true:
								set {hitdelay.%attacker%.%victim%} to {attackdelay.%attacker%}
								set maximum damage delay of victim to 0.1 seconds
								if attacker's tool is iron axe:
									set damage to damage + {버서커추가뎀.%attacker%}
								chance of 30:
									set {냉술사슬로우.%victim%} to 3
								stop
						if {hitdelay.%attacker%.%victim%} > 0:
							cancel event
			else:
				set maximum damage delay of victim to 0.1 seconds

every 0.1 seconds:
	loop all players:
		loop all players:
			if {hitdelay.%loop-player-2%.%loop-player-1%} > 0:
