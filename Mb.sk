#변수 설정용 명령어
command /variable [<text>] [<text>] [<text>] [<text>]:
	trigger:
		if arg 1 is "set":
			if arg 4 is set:
				set {%arg 2%.%arg 4%} to "%arg 3%"
			if arg 4 is not set:
				set {%arg 2%} to "%arg 3%"
				stop
		if arg 1 is "add":
			if arg 4 is set:
				add arg 3 to {%arg 2%.%arg 4%}
			if arg 4 is not set:
				add arg 3 to {%arg 2%}
				stop
		if arg 1 is "message":
			if arg 3 is set:
				message "%{%arg 2%.%arg 3%}% 입니다"
			if arg 3 is not set:
				message "%{%arg 2%}% 입니다"
		if arg 1 is not set:
			message "/variable [set/add/message] [변수] [값] [플레이어]"


command /gm [<integer>] [<string>]:
	trigger:
		if arg 2 is not set:
			if arg 1 is 1:	
				execute player command "/gamemode creative %player%"
			if arg 1 is 0:	
				execute player command "/gamemode survival %player%"
			if arg 1 is 2:	
				execute player command "/gamemode adventure %player%"
			if arg 1 is 3:	
				execute player command "/gamemode spectator %player%"
			stop
		if arg 2 is set:
			if arg 1 is 1:
				execute player command "/gamemode creative %arg 2%"
			if arg 1 is 0:	
				execute player command "/gamemode survival %arg 2%"
			if arg 1 is 2:	
				execute player command "/gamemode adventure %arg 2%"
			if arg 1 is 3:	
				execute player command "/gamemode spectator %arg 2%"
				
command /tpall:
	trigger:
		execute player command "/tp @a %player%"
		

command /flyoff:
	trigger:
		message "fly off"
		make player stop flying
		
		
command /flyon:
	trigger:
		message "fly on"
		make player start flying
		



